{
  "name": "zeta-dom",
  "version": "0.5.1",
  "description": "",
  "type": "module",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "unpkg": "dist/zeta.min.js",
  "scripts": {
    "build": "webpack",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:dts": "npx check-dts tests/**/*.ts",
    "version": "npm run build && git add -A dist",
    "release": "npx @misonou/build-utils publish"
  },
  "author": "misonou",
  "license": "ISC",
  "homepage": "https://misonou.github.io",
  "repository": "github:misonou/zeta-dom",
  "dependencies": {
    "jquery": "^3.0.0"
  },
  "optionalDependencies": {
    "promise-polyfill": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@jest/globals": "^26.6.2",
    "@misonou/build-utils": "^1.3.1",
    "@misonou/test-utils": "^1.0.7",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^26.0.15",
    "@types/jquery": "^3.5.27",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.2",
    "expect-type": "^0.17.3",
    "jest": "^27.0.6",
    "syn": "^0.15.0"
  },
  "exports": {
    ".": "./src/index.js",
    "./*": "./src/*.js"
  },
  "umdExports": {
    "index": "lib",
    "cssUtil": "lib.css",
    "dom": "lib.dom",
    "domLock": "lib.dom",
    "domUtil": "lib.util",
    "env": "lib",
    "errorCode": "lib.ErrorCode",
    "events": {
      "path": "lib",
      "names": {
        "ZetaEventContainer": "EventContainer",
        "ZetaEventSource": "EventSource"
      }
    },
    "observe": "lib.dom",
    "tree": "lib",
    "util": "lib.util"
  },
  "browserslist": [
    "> 1%",
    "not dead",
    "ie >= 11"
  ],
  "keywords": [
    "browser",
    "html",
    "dom",
    "virtual dom",
    "util",
    "utility",
    "components"
  ]
}
